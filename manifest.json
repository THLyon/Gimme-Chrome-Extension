{
    "name": "Gimme Chrome Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": [
             "./src/index.js" 
          ],
          "run_at": "document_start"
        }
      ],
      "action": {
        "default_popup": "./src/Popup/Popup.html",
        "default_title": "Gimme"
      }
  }